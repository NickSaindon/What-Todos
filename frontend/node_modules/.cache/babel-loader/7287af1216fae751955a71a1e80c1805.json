v class=\"add-todo-header\">\n      <router-link to=\"/\">\n        <i class=\"bi bi-arrow-left\"> back to what todos</i>\n      </router-link>\n    </div>\n    <h3>Add What Todo</h3>\n    <div class=\"add\">\n      <form class=\"row g-3\" @submit=\"onSubmit\">\n        <div class=\"col-md-12\">\n          <label for=\"category\" class=\"form-label\">Category</label>\n          <select v-model=\"category\" class=\"form-select\">\n            <option disabled value>Select a Category</option>\n            <option value=\"Personal\">Personal</option>\n            <option value=\"Business\">Business</option>\n          </select>\n        </div>\n        <div class=\"col-md-12\">\n          <label for=\"title\" class=\"form-label\">Title</label>\n          <input\n            v-model=\"title\"\n            type=\"text\"\n            class=\"form-control\"\n            id=\"title\"\n            placeholder=\"Title of what todo\"\n          />\n        </div>\n        <div class=\"col-md-12\">\n          <label for=\"description\" class=\"form-label\">\n            Schedule Date and Time\n          </label>\n          <Datepicker\n            v-model=\"scheduled\"\n            :is24=\"false\"\n            placeholder=\"Select Date and Time\"\n          />\n        </div>\n        <div class=\"col-md-12\">\n          <label for=\"description\" class=\"form-label\">Description</label>\n          <textarea\n            v-model=\"description\"\n            class=\"form-control\"\n            placeholder=\"Describe what you will be doing\"\n            rows=\"3\"\n          >\n          </textarea>\n        </div>\n        <div class=\"col-md-12\">\n          <div class=\"add-todo-footer\">\n            <div class=\"d-grid gap-2 col-6 mx-auto\">\n              <button type=\"submit\" value=\"Submit\" class=\"btn btn-primary\">\n                Submit What Todo\n              </button>\n            </div>\n          </div>\n        </div>\n      </form>\n      <p>{{category}}</p>\n      <p>{{title}}</p>\n      <p>{{scheduled.toString}}</p>\n      <p>{{description}}</p>\n\n\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapActions } from \"vuex\";\nimport Datepicker from \"vue3-date-time-picker\";\n\nexport default {\n  name: \"AddWhatTodo\",\n  components: {\n    Datepicker,\n  },\n\n  data() {\n    return {\n      category: \"\",\n      title: \"\",\n      description: \"\",\n      scheduled: \"\",\n    };\n  },\n  methods: {\n    ...mapActions([\"addTodo\"]),\n    onSubmit(event) {\n      event.preventDefault();\n      this.addTodo(this.category);\n      this.addTodo(this.title);\n      this.addTodo(this.description);\n      this.addTodo(this.scheduled.toString);\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"scss\">\n.add-todo-header {\n  padding: 20px 0;\n}\n.add-todo-footer {\n  padding: 20px 0;\n}\n</style>\n"],"sourceRoot":""}]}
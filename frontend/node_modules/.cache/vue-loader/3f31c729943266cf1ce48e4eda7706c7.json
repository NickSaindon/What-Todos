ot":"","sourcesContent":["<template>\n\n      <div class=\"container\">\n<button type=\"button\" class=\"btn btn-primary\">Add What Todo</button>\n        <div class=\"row row-cols-1 row-cols-sm-1 row-cols-md-1 row-cols-lg-1 row-cols-xl-2\">\n          <div\n            v-for=\"todo in allTodos\"\n            :key=\"todo.id\"\n            @dblclick=\"onDoubleClick(todo)\"\n            class=\"todo\"\n            v-bind:class=\"{'is-complete': todo.completed}\"\n          >\n            <div class=\"d-xl-flex align-items-stretch\">\n                <div class=\"card\">\n                <div class=\"row\">\n                    <div class=\"col-2 cat-container\">\n                    <div class=\"circle\">\n                        <i class=\"bi bi-person-fill\"></i>                                    \n                    </div>\n                    </div>\n                    <div class=\"col-8\">\n                    <div class=\"card-body\">\n                        <h5 class=\"card-title\">{{todo.title}}</h5>\n                        <p class=\"card-text\">{{todo.description}}</p>\n                        <p class=\"card-text\"><small class=\"text-muted\">{{todo.scheduled}}</small></p>\n                    </div>\n                    </div>\n                    <div class=\"col-2\">\n                    <div class=\"action-btns\">\n                        <button class=\"btn btn-success\" type=\"button\">\n                        <i class=\"bi bi-check-lg\"></i>\n                        </button>\n                        <button class=\"btn btn-danger\" type=\"button\">\n                        <i class=\"bi bi-trash-fill\"></i>\n                        </button>\n                    </div>\n                    </div>\n                </div>\n                </div>\n            </div>\n          </div>\n        </div>\n      </div>\n</template>\n\n<script>\nimport { mapGetters, mapActions } from \"vuex\";\n\nexport default {\n    name: \"WhatTodos\",\n    methods: {\n        ...mapActions([\n            'fetchTodos',\n            'deleteTodo',\n            'updateTodo'\n        ]),\n        onDoubleClick(currentTodo) {\n            const updatedTodo = {\n                id: currentTodo.id,\n                category: currentTodo.category,\n                title: currentTodo.title,\n                scheduled: currentTodo.scheduled,\n                completed: !currentTodo.completed\n            }\n            this.updateTodo(updatedTodo);\n        }\n    },\n    computed: {\n        ...mapGetters([\n            'allTodos',\n        ])\n    },\n    created() {\n        this.fetchTodos();\n    }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\n    .todos-container {\n        width: 100%;\n        height: auto;\n        border: 1px solid rgba(0, 0, 0, 0.125);\n        border-radius: 5px;\n        background: #f7f7f7;        \n        margin: 200px 0 80px 0;\n    }\n\n    .card {\n        margin: 10px 0;\n    }\n\n    .action-btns {\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        flex-direction: column;\n        height: 100%;\n        button {\n            margin: 5px 10px;\n        }\n    }\n\n    .cat-container {\n            display: flex;\n            justify-content: center;\n            align-items: center;\n        .circle {\n            height: 40px;\n            width: 40px;\n            border: 1px solid #0d6efd;\n            border-radius: 50%;\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            i {\n                font-size: 20px;\n                color: #0d6efd;\n            }\n        }\n    }\n\n</style>"]}]}
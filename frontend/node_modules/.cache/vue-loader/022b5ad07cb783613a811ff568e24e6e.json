      placeholer=\"Title\"\n          />\n        </div>\n        <div class=\"col-md-6\">\n          <label for=\"description\" class=\"form-label\">Description</label>\n          <textarea class=\"form-control\" v-model=\"description\" rows=\"3\">\n          </textarea>\n        </div>\n        <div class=\"col-12\">\n          <button type=\"submit\" class=\"btn btn-primary\">Sign in</button>\n        </div>\n      </form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapActions } from \"vuex\";\n\nexport default {\n  name: \"AddWhatTodo\",\n  data() {\n    return {\n      category: \"\",\n      title: \"\",\n      description: \"\",\n      scheduled: \"\",\n    };\n  },\n  methods: {\n    ...mapActions([\"addTodo\"]),\n    onSubmit(event) {\n      event.preventDefault();\n      this.addTodo(this.category);\n      this.category = \"\";\n      this.addTodo(this.title);\n      this.title = \"\";\n      this.addTodo(this.description);\n      this.description = \"\";\n      this.addTodo(this.scheduled);\n      this.scheduled = \"\";\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"scss\">\n.add-todo-header {\n  padding: 20px 0;\n}\n</style>\n"]}]}